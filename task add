		public class Z
		{

		public void m1()
		{
		int a =10;
		int b =20;
		int c= a+b;
		System.out.println("sum in withour parametr methdo = " +c);
		}

		public int m2()
		{
		int a =10;
		int b =20;
		int c= a+b;
		return c;
		}
		public static void main(String[] args)
		{
		Z a = new Z();
		a.m1();
		int h = a.m2();
		System.out.println("from return type method sum is "+ h);

		}


		}
